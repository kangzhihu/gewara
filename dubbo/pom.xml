		</pluginRepository> </pluginRepositories> <distributionManagement> <repository> 
		<id>gewara-release</id> <url>http://192.168.2.224:8081/nexus/content/repositories/gewara-release</url> 
		</repository> <snapshotRepository> <id>gewara-snapshot</id> <url>http://192.168.2.224:8081/nexus/content/repositories/gewara-snapshot</url> 
		</snapshotRepository> </distributionManagement> -->
</project>